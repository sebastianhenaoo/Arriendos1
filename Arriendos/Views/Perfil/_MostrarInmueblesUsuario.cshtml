@model IEnumerable<Arriendos.Models.Propiedad>
    @{
        ViewBag.Title = "Index";
    }

 <center>
    <h2 class="titulo-prop">Mis propiedades</h2>
</center>
<br>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Direccion)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
        </tr>
    }

</table>


